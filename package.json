{
  "name": "vue-recoil",
  "version": "0.0.5",
  "description": "A state management library for Vue3",
  "main": "dist/recoil.cjs.js",
  "module": "dist/recoil.es.js",
  "unpkg": "dist/recoil.umd.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": "https://github.com/lokeezZZ/vue-recoil.git",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c  && node scripts/postbuild.js",
